<MudDialog>
    <DialogContent>
        <MudCardMedia Style="width: 300px; height: 200px;object-position:center;object-fit:scale-down"
                      Image="@SelectedItem.ImageBlobUrl" />
    </DialogContent>
</MudDialog>

@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; } = null!;
    [Parameter] public Add SelectedItem { get; set; } = new();
}
